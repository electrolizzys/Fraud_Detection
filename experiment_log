 = X.select_dtypes(include=['object', 'category']).columns\n        for col in cat_cols:\n            n_unique = X[col].nunique(dropna=False)\n            if n_unique <= self.max_unique_for_onehot:\n                self.low_cardinality_cols_.append(col)\n            else:\n                self.high_cardinality_cols_.append(col)\n        return self\n\n    def transform(self, X):\n        return X  \n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-04-26T16:43:05.364678Z","iopub.execute_input":"2025-04-26T16:43:05.365530Z","iopub.status.idle":"2025-04-26T16:43:05.377427Z","shell.execute_reply.started":"2025-04-26T16:43:05.365497Z","shell.execute_reply":"2025-04-26T16:43:05.376366Z"}},"outputs":[],"execution_count":323},{"cell_type":"code","source":"def droptoomanynulls(threshold=0.8):\n    return DropNulls(threshold=threshold)\n","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-04-26T16:43:09.705143Z","iopub.execute_input":"2025-04-26T16:43:09.705940Z","iopub.status.idle":"2025-04-26T16:43:09.710271Z","shell.execute_reply.started":"2025-04-26T16:43:09.705908Z","shell.execute_reply":"2025-04-26T16:43:09.709228Z"}},"outputs":[],"execution_count":324},{"cell_type":"code","source":"from sklearn.compose import make_column_selector \n\ndef woeandcathandling(max_unique_for_onehot=3):\n    numerical_transformer = SimpleImputer(strategy='median')\n    onehot_encoder = OneHotEncoder(handle_unknown='ignore', sparse_output=False)\n    woe_encoder = ce.WOEEncoder()\n\n    splitter = SplitCatCols(max_unique_for_onehot=max_unique_for_onehot)\n    return Pipeline(steps=[\n        ('split_cat', splitter),\n        ('encode', ColumnTransformer(transformers=[\n            ('num', numerical_transformer, make_column_selector(dtype_include=['int64', 'float64'])),\n            ('onehot', onehot_encoder, make_column_selector(dtype_include=['object', 'category'])),\n            ('woe', woe_encoder, make_column_selector(dtype_include=['object', 'category'])